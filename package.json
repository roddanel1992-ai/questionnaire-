{
  "name": "aws-dva-c02-quiz",
  "version": "2.0.0",
  "description": "AWS DVA-C02 Certification Practice Exam - Production Ready for 1M+ Users",
  "main": "app.js",
  "scripts": {
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:unit": "jest tests/unit",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:all": "npm run test:unit && npm run test:e2e",
    "serve": "python -m http.server 8000",
    "lint": "eslint app.js",
    "validate": "npm run lint && npm run test:all"
  },
  "keywords": [
    "aws",
    "dva-c02",
    "certification",
    "quiz",
    "exam",
    "practice"
  ],
  "author": "ROD",
  "license": "MIT",
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@types/jest": "^29.5.0",
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "collectCoverageFrom": [
      "app.js",
      "!node_modules/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  }
}

